function openZip()
    set error to 0
    for i to array.length
        set path
        open archive with path
        if archive is null
            print error
            exit
        end if

        get file info
        create and read txt

        open encrypted file
        if file descriptor is null
            print error
            close archive
            exit
        end if

        read content
        close file descriptor
    end loop
